FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      k;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -2 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    lateral_boundary
    {
        type            zeroGradient;
    }

    
    orifice
    {
        type            fixedValue;
	value		uniform 0;
    }
    
	
    inflow_boundary
    {
        type            fixedValue;
        value           uniform 0;
    }

    outflow_boundary
    {
        type            zeroGradient;
    }
}


